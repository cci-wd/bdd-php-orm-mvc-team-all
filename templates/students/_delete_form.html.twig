<form method="post" action="{{ path('students_delete', {'idStudents': student.idStudents}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ student.idStudents) }}">
    <button class="btn">Delete</button>
</form>
